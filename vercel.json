{
  "buildCommand": "cd apps/web && pnpm build",
  "installCommand": "pnpm install",
  "devCommand": "cd apps/web && pnpm dev",
  "framework": "nextjs",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/en/?",
      "dest": "/login"
    },
    {
      "src": "/pt-BR/?",
      "dest": "/login"
    },
    {
      "src": "^/$",
      "dest": "/login"
    }
  ]
}
